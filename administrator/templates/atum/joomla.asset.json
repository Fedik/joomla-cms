{
  "$schema": "https://developer.joomla.org/schemas/json-schema/web_assets.json",
  "name": "atum",
  "version": "4.0.0",
  "description": "Atum is the Joomla 4 administrator template",
  "license": "GPL-2.0+",
  "assets": [
    {
      "name": "bootstrap.css",
      "type": "stylesheet",
      "uri": "bootstrap.min.css"
    },
    {
      "name": "fontawesome",
      "type": "stylesheet",
      "uri": "fontawesome.min.css"
    },
    {
      "name": "template.atum.ltr",
      "type": "stylesheet",
      "uri": "template.min.css"
    },
    {
      "name": "template.atum.rtl",
      "type": "stylesheet",
      "uri": "template-rtl.min.css"
    },
    {
      "name": "template.atum.user",
      "type": "stylesheet",
      "uri": "user.css"
    },
    {
      "name": "template.atum.base",
      "type": "group",
      "dependencies": [
        "core#script",
        "fontawesome#stylesheet",
        "bootstrap.css#stylesheet",
        "css-vars-ponyfill#script"
      ]
    },
    {
      "name": "template.atum.ltr",
      "type": "group",
      "dependencies": [
        "template.atum.base",
        "template.atum.ltr#stylesheet",
        "template.atum.user#stylesheet"
      ]
    },
    {
      "name": "template.atum.rtl",
      "type": "group",
      "dependencies": [
        "template.atum.base",
        "template.atum.rtl#stylesheet",
        "template.atum.user#stylesheet"
      ]
    }
  ]
}
